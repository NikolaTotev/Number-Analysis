Cell[TextData[{
 "Creating the R(x) polynomial based on the formula.",
 "\n",
 Cell[BoxData[
  StyleBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"R", "[", "x_", "]"}], " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "6"}], "/", 
         RowBox[{"(", 
          SuperscriptBox["x", "4"], ")"}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "+", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "-", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "+", "0.3"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "-", "0.3"}], ")"}]}]}], ";"}], " "}], "Input"]], "Input",
  CellChangeTimes->{{3.7833548726189337`*^9, 3.783354876717171*^9}, {
   3.7833575681714444`*^9, 3.783357644222443*^9}, {3.783358354634692*^9, 
   3.783358489110407*^9}},
  Background->RGBColor[1, 0.9, 0.8],ExpressionUUID->
  "0d4e80fb-4fc2-446f-9ef6-2666ca5c3818"]
}], "Text",
